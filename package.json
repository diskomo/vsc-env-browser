{
  "name": "env-browser",
  "displayName": "Env Browser",
  "version": "0.0.3",
  "publisher": "METAIIII",
  "description": "Provides a UI for managing `.env` files.",
  "author": {
    "name": "METAIIII"
  },
  "categories": [
    "Other"
  ],
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#0F0F0F",
    "theme": "dark"
  },
  "pricing": "Free",
  "engines": {
    "vscode": "^1.75.0"
  },
  "main": "./out/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "envBrowser.envEditor",
        "displayName": "Env Browser",
        "selector": [
          {
            "filenamePattern": ".env*"
          }
        ],
        "priority": "default"
      }
    ],
    "commands": [
      {
        "command": "envBrowser.toggleView",
        "title": "Env Browser: Toggle View",
        "icon": "$(file-text)"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./ && npm run bundle-webview && npm run copy-assets",
    "bundle-webview": "esbuild src/webview/env-table.ts --bundle --format=esm --outfile=out/webview/env-table.js --target=es2022 --platform=browser",
    "copy-assets": "node scripts/copy-assets.js",
    "watch": "tsc -watch -p ./",
    "package": "vsce package",
    "lint": "biome check .",
    "format": "biome format --write ."
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.10",
    "@types/node": "^25.0.3",
    "@types/vscode": "^1.75.0",
    "@vscode/vsce": "^3.7.1",
    "esbuild": "^0.27.2",
    "typescript": "^5.9.3"
  },
  "bugs": {
    "url": "https://github.com/diskomo/vsc-env-browser/issues",
    "email": "dev@metaiiii.online"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/diskomo/vsc-env-browser.git"
  },
  "homepage": "https://github.com/diskomo/vsc-env-browser/blob/main/README.md",
  "license": "MIT"
}
